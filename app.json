    "description": "An Advanced Premium Music Bot For Telegram Groups VC.",
    "keywords": ["music", "voice chat", "telegram"],
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "Your App ID",
            "required": true
        },
        "API_HASH": {
            "description": "Your App Hash",
            "required": true
        },
        "STRING_SESSION": {
            "description": "Pyrogram String Session",
            "required": true
        },
        "HEROKU_API": {
            "description": "Enter You Heroku Api there",
            "required": false
        },
        "HEROKU_APP_NAME": {
            "description": "Enter Your heroku App Name Here",
            "required": false        
